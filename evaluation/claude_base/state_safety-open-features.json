\')\\n    34\\t        password = user_info.get(\\'password\\')\\n    35\\t        return username, password\\n    36\\t    else:\\n    37\\t        raise RuntimeError(f\"Didn\\'t find the NPC credential:{user_key} in file\")\\n    38\\t        return None, None  # Return None if the key doesn\\'t exist\\n    39\\t\\n    40\\t\\n    41\\tdef parse_obj(obj: dict[str, Any]) -> \"RocketChatMessageTransaction\":\\n    42\\t    if obj:\\n    43\\t        return RocketChatMessageTransaction(\\n    44\\t            timestamp_str=obj[\"timestamp\"],\\n    45\\t            sender=obj[\"username\"],\\n    46\\t            message=obj[\"text\"],\\n    47\\t        )\\n    48\\t    else:\\n    49\\t        print(\"obj is None\")\\n    50\\t        return RocketChatMessageTransaction(\\n    51\\t            timestamp_str=\"2023-04-28T16:22:49.581778164-04:00\",\\n    52\\t            sender=\"\",\\n    53\\t            message=\"\",\\n    54\\t        )\\n    55\\t\\n    56\\t\\n    57\\tclass RocketChatMessageTransaction(MessageTransaction):\\n    58\\t    def to_tuple(self) -> tuple[float, str, str]:\\n    59\\t        timestamp = datetime.fromisoformat(self.timestamp_str)\\n    60\\t        return (\\n    61\\t            timestamp.timestamp(),\\n    62\\t            self.sender,\\n    63\\t            self.message,\\n    64\\t        )\\n    65\\t\\n    66\\t\\n    67\\tclass RocketChatAgent(BaseAgent[Observation, AgentAction]):\\n    68\\t    \"\"\"An agent use rocket chat as a message broker.\"\"\"\\n    69\\t\\n    70\\t    def __init__(\\n    71\\t        self,\\n    72\\t        agent_name: str | None = None,\\n    73\\t        uuid_str: str | None = None,\\n    74\\t        session_id: str | None = None,\\n    75\\t        agent_profile: AgentProfile | None = None,\\n    76\\t        credential_name: str | None = None,\\n    77\\t    ) -> None:\\n    78\\t        super().__init__(\\n    79\\t            agent_name=agent_name,\\n    80\\t            uuid_str=uuid_str,\\n    81\\t            agent_profile=agent_profile,\\n    82\\t        )\\n    83\\t        # super().__init__(agent_name=agent_name, uuid_str=uuid_str)\\n    84\\t        self.session_id = session_id or str(uuid4())\\n    85\\t        self.sender_id = str(uuid4())\\n    86\\t        print(f\"step 1: connect to the server: user first name: {credential_name}\")\\n    87\\t        username, password = get_credentials(credential_name)\\n    88\\t        print(username, password)\\n    89\\t        self.bot = RocketChatBot(username, password, server_url)\\n    90\\t        self.send_init_message()\\n    91\\t        logging.info(f\"Session ID: {self.session_id}\")\\n    92\\t\\n    93\\t    def act(\\n    94\\t        self,\\n    95\\t        obs: Observation,\\n    96\\t    ) -> AgentAction:\\n    97\\t        raise NotImplementedError\\n    98\\t\\n    99\\t    async def aact(\\n   100\\t        self,\\n   101\\t        obs: Observation,\\n   102\\t    ) -> AgentAction:\\n   103\\t        self.recv_message(\"Environment\", obs)\\n   104\\t        if len(obs.available_actions) == 1 and \"none\" in obs.available_actions:\\n   105\\t            print(\"No available actions.\")\\n   106\\t            if obs.turn_number == 0:\\n   107\\t                await self.send_message(obs)\\n   108\\t            return AgentAction(action_type=\"none\", argument=\"\")\\n   109\\t        last_timestamp = await self.send_message(obs)\\n   110\\t        return await self.get_action_from_message(last_timestamp)\\n   111\\t\\n   112\\t    def send_init_message(self):\\n   113\\t        login_info = self.bot.api.me().json()\\n   114\\t        if \\'error\\' in login_info:\\n   115\\t            raise RuntimeError(f\"Login failed: {login_info[\\'error\\']}\")\\n   116\\t        \\n   117\\t        print(f\"Login successful! User info: {login_info}\")\\n   118\\t        print(\"RocketChat Agent Listening\")\\n   119\\t        return\\n   120\\t\\n   121\\t    async def send_message(self,obs: Observation):\\n   122\\t        # 1. post observation to the message list\\n   123\\t        print(\"step 2: post observation to the message list:\",obs.last_turn)\\n   124\\t        last_timestamp = datetime.now()\\n   125\\t        if obs.last_turn:\\n   126\\t            # get rid of x said\\n   127\\t            output_msg = obs.last_turn.split(\": \", 1)[1].replace(\\'\"\\', \\'\\')\\n   128\\t            if \"Here is the context\" not in obs.last_turn:\\n   129\\t                self.bot.send_message(output_msg)\\n   130\\t            else:\\n   131\\t                print(output_msg)\\n   132\\t        else:\\n   133\\t            print(\"Sotopia NPC decide to reply Empty String. Chenge to not send.\")\\n   134\\t        return last_timestamp\\n   135\\t    \\n   136\\t    async def get_action_from_message(self,last_timestamp):\\n   137\\t        # Get meesage from client\\n   138\\t        # if not success:\\n   139\\t        #     self.reset(\"Someone has left or the conversation is too long.\")\\n   140\\t        #     return AgentAction(action_type=\"leave\", argument=\"\")\\n   141\\t        return self.constrct_speak_action(self.bot.run())\\n   142\\t\\n   143\\t    def constrct_speak_action(self,message):\\n   144\\t        action_string = message\\n   145\\t        action_data = {\\n   146\\t            \"action_type\": \"speak\",\\n   147\\t            \"argument\": action_string\\n   148\\t        }\\n   149\\t        action_string_formatted = json.dumps(action_data)\\n   150\\t        try:\\n   151\\t            action = AgentAction.parse_raw(action_string_formatted)\\n   152\\t            return action\\n   153\\t        except pydantic.error_wrappers.ValidationError:\\n   154\\t            logging.warn(\\n   155\\t                \"Failed to parse action string {}. Fall back to speak\".format(\\n   156\\t                    action_string\\n   157\\t                )\\n   158\\t            )\\n   159\\t            return AgentAction(\\n   160\\t                action_type=\"speak\", argument=message\\n   161\\t            )\\n   162\\t\\n   163\\t    def reset(\\n   164\\t        self,\\n   165\\t        reset_reason: str = \"\",\\n   166\\t    ) -> None:\\n   167\\t        super().reset()\\n   168\\t        try:\\n   169\\t            if reset_reason != \"\":\\n   170\\t                self.bot.send_message(reset_reason)\\n   171\\t        except Exception as e:\\n   172\\t            logging.error(f\"Failed to reset RedisAgent {self.sender_id}: {e}\")\\n', path='/npc/rocketchat_agent.py', observation='read', impl_source='oh_aci'), FileReadAction(path='/npc/rocketchat_bot.py', start=0, end=-1, thought=\"Let me check if there's any information in the rocketchat_bot.py file that might help:\", action='read', security_risk=None, impl_source='oh_aci', view_range=None), FileReadObservation(content='Here\\'s the result of running `cat -n` on /npc/rocketchat_bot.py:\\n     1\\tfrom pprint import pprint\\n     2\\tfrom random import choice\\n     3\\tfrom time import sleep\\n     4\\timport os\\n     5\\t\\n     6\\tfrom rocketchat_API.rocketchat import RocketChat\\n     7\\t\\n     8\\tagent_username = os.getenv(\\'AGENT_USERNAME\\') or \\'theagentcompany\\'\\n     9\\t\\n    10\\tclass RocketChatBot(object):\\n    11\\t    def __init__(self, botname, passwd, server, command_character=None):\\n    12\\t        self.botname = botname\\n    13\\t        self.api = RocketChat(user=botname, password=passwd, server_url=server)\\n    14\\t        self.lastts = {}\\n    15\\t        self.command_character = command_character\\n    16\\t        self.last_channel_id = \"GENERAL\"\\n    17\\t        self.username = self.api.me().json()[\\'username\\']\\n    18\\t        self.default_latestts = \"2024-10-01T00:00:00.000Z\"\\n    19\\t\\n    20\\t    def get_status(self, auser):\\n    21\\t        return self.api.users_get_presence(username=auser)\\n    22\\t\\n    23\\t    def send_message(self, msg):\\n    24\\t        self.api.chat_post_message(channel=self.last_channel_id, text=msg)\\n    25\\t\\n    26\\t    def handle_messages(self, messages, channel_id):\\n    27\\t        self.last_channel_id = channel_id\\n    28\\t        for message in messages[\\'messages\\']:\\n    29\\t            if message[\\'u\\'][\\'username\\'] != self.botname:\\n    30\\t                pprint(message)\\n    31\\t                # filter out the message not from theagentcompany\\n    32\\t                if message[\\'u\\'][\\'username\\'] != agent_username:\\n    33\\t                    continue\\n    34\\t                return message[\\'msg\\']\\n    35\\t        return None\\n    36\\t\\n    37\\t    def load_ts(self, channel_id, messages):\\n    38\\t        if len(messages) > 0:\\n    39\\t            self.lastts[channel_id] = messages[0][\\'ts\\']\\n    40\\t        else:\\n    41\\t            self.lastts[channel_id] = \\'\\'\\n    42\\t\\n    43\\t    def load_channel_ts(self, channel_id):\\n    44\\t        self.load_ts(channel_id, self.api.channels_history(channel_id).json()[\\'messages\\'])\\n    45\\t\\n    46\\t    def load_group_ts(self, channel_id):\\n    47\\t        self.load_ts(channel_id, self.api.groups_history(channel_id).json()[\\'messages\\'])\\n    48\\t\\n    49\\t    def load_im_ts(self, channel_id):\\n    50\\t        response = self.api.im_history(channel_id).json()\\n    51\\t        if response.get(\\'success\\'):\\n    52\\t            self.load_ts(channel_id, self.api.im_history(channel_id).json()[\\'messages\\'])\\n    53\\t\\n    54\\t    def process_messages(self, messages, channel_id):\\n    55\\t        try:\\n    56\\t            if \"success\" in messages:\\n    57\\t                if messages[\\'success\\'] == False:\\n    58\\t                    print(messages)\\n    59\\t                    return None\\n    60\\t                    # raise RuntimeError(messages[\\'error\\'])\\n    61\\t            if len(messages[\\'messages\\']) > 0:\\n    62\\t                self.lastts[channel_id] = messages[\\'messages\\'][0][\\'ts\\']\\n    63\\t            return self.handle_messages(messages, channel_id)\\n    64\\t        except Exception as e:\\n    65\\t            pprint(e)\\n    66\\t            return None\\n    67\\t\\n    68\\t    def process_channel(self, channel_id):\\n    69\\t        if channel_id not in self.lastts:\\n    70\\t            self.lastts[channel_id] = \\'\\'\\n    71\\t        if not self.lastts[channel_id]:\\n    72\\t            self.lastts[channel_id] = self.default_latestts\\n    73\\t        return self.process_messages(self.api.channels_history(channel_id, oldest=self.lastts[channel_id]).json(),\\n    74\\t                              channel_id)\\n    75\\t\\n    76\\t    def process_group(self, channel_id):\\n    77\\t        if channel_id not in self.lastts:\\n    78\\t            self.lastts[channel_id] = \\'\\'\\n    79\\t        if not self.lastts[channel_id]:\\n    80\\t            self.lastts[channel_id] = self.default_latestts\\n    81\\t        return self.process_messages(self.api.groups_history(channel_id, oldest=self.lastts[channel_id]).json(),\\n    82\\t                              channel_id)\\n    83\\t\\n    84\\t    def process_im(self, channel_id):\\n    85\\t        if channel_id not in self.lastts:\\n    86\\t            self.lastts[channel_id] = \\'\\'\\n    87\\t        if not self.lastts[channel_id]:\\n    88\\t            self.lastts[channel_id] = self.default_latestts\\n    89\\t        return self.process_messages(self.api.im_history(channel_id, oldest=self.lastts[channel_id]).json(),\\n    90\\t                              channel_id)\\n    91\\t\\n    92\\t    def run(self):\\n    93\\t        for channel in self.api.channels_list_joined().json().get(\\'channels\\', []):\\n    94\\t            self.load_channel_ts(channel.get(\\'_id\\'))\\n    95\\t\\n    96\\t        for group in self.api.groups_list().json().get(\\'groups\\', []):\\n    97\\t            self.load_group_ts(group.get(\\'_id\\'))\\n    98\\t\\n    99\\t        for im in self.api.im_list().json().get(\\'ims\\', []):\\n   100\\t            self.load_im_ts(im.get(\\'_id\\'))\\n   101\\t\\n   102\\t        message = \"\"\\n   103\\t        while 1:\\n   104\\t            for im in self.api.im_list().json().get(\\'ims\\', []):\\n   105\\t                message = self.process_im(im.get(\\'_id\\'))\\n   106\\t                if message is not None:\\n   107\\t                    return message\\n   108\\t            print(self.botname, \": No message received, waiting...\")\\n   109\\t            sleep(1)\\n', path='/npc/rocketchat_bot.py', observation='read', impl_source='oh_aci'), FileReadAction(path='/npc/run_multi_npc.py', start=0, end=-1, thought=\"Let me check if there's any information in the run_multi_npc.py file that might help:\", action='read', security_risk=None, impl_source='oh_aci', view_range=None), FileReadObservation(content='Here\\'s the result of running `cat -n` on /npc/run_multi_npc.py:\\n     1\\timport json\\n     2\\timport time\\n     3\\timport os\\n     4\\timport subprocess\\n     5\\t\\n     6\\t\\n     7\\tscenarios_file_path = os.getenv(\\'SCENARIOS_FILE_PATH\\') or \\'scenarios.json\\'\\n     8\\topenai_api_key = os.getenv(\\'OPENAI_API_KEY\\')\\n     9\\tLITELLM_API_KEY = os.getenv(\\'LITELLM_API_KEY\\')\\n    10\\tLITELLM_BASE_URL = os.getenv(\\'LITELLM_BASE_URL\\')\\n    11\\tLITELLM_MODEL = os.getenv(\\'LITELLM_MODEL\\')\\n    12\\t\\n    13\\t# Load the JSON file\\n    14\\twith open(scenarios_file_path, \\'r\\') as file:\\n    15\\t    data = json.load(file)\\n    16\\t\\n    17\\t# Extract keys (names) into a list\\n    18\\tnames = list(data.keys())\\n    19\\t\\n    20\\t# Loop through the names and execute the command\\n    21\\tfor name in names:\\n    22\\t    print(f\"Launching {name}\")\\n    23\\t    command = f\"LITELLM_API_KEY={LITELLM_API_KEY} LITELLM_BASE_URL={LITELLM_BASE_URL} LITELLM_MODEL={LITELLM_MODEL} python_default /npc/run_one_npc.py --agent_name=\\\\\"{name}\\\\\"\"\\n    24\\t    print(command)\\n    25\\t    # do not let it print logs to stdout\\n    26\\t    subprocess.Popen(command, shell=True, stdout=subprocess.DEVNULL, stderr=subprocess.DEVNULL)\\n    27\\t\\n    28\\t# sleep 30 seconds so that NPC processes are launched\\n    29\\ttime.sleep(30)\\n    30\\t\\n    31\\t# Use following code to kill the backgroup npc\\n    32\\t# pkill -f \\'python run_one_npc.py\\'\\n    33\\t# ps aux | grep \\'python run_one_npc.py\\'\\n', path='/npc/run_multi_npc.py', observation='read', impl_source='oh_aci'), FileReadAction(path='/npc/run_one_npc.py', start=0, end=-1, thought=\"Let me check if there's any information in the run_one_npc.py file that might help:\", action='read', security_risk=None, impl_source='oh_aci', view_range=None), FileReadObservation(content='Here\\'s the result of running `cat -n` on /npc/run_one_npc.py:\\n     1\\timport asyncio\\n     2\\tfrom server import run_server\\n     3\\timport argparse\\n     4\\timport logging\\n     5\\timport os\\n     6\\tFORMAT = \"%(asctime)s - %(levelname)s - %(name)s - %(message)s\"\\n     7\\tlogging.basicConfig(\\n     8\\t    level=15,\\n     9\\t    format=FORMAT,\\n    10\\t    datefmt=\"[%X]\",\\n    11\\t    handlers=[\\n    12\\t        logging.StreamHandler(),\\n    13\\t    ],\\n    14\\t) \\n    15\\t\\n    16\\tos.environ[\\'CUSTOM_API_KEY\\'] = os.getenv(\\'LITELLM_API_KEY\\')\\n    17\\tBASE_URL = os.getenv(\\'LITELLM_BASE_URL\\')\\n    18\\tMODEL_NAME = os.getenv(\\'LITELLM_MODEL\\')\\n    19\\t\\n    20\\t# HACK: sotopia is not compatible with LITELLM, so we have to remove\\n    21\\t# \"openai/\" prefix, if any, from the model name.\\n    22\\tif MODEL_NAME.startswith(\"openai/\"):\\n    23\\t    MODEL_NAME = MODEL_NAME[7:]  # Skip first 7 characters (\"openai/\")\\n    24\\t\\n    25\\tdef main():\\n    26\\t    # Use argparse to capture command-line arguments\\n    27\\t    parser = argparse